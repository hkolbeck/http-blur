  "action": {
    "default_icon": {
      "16": "images/SocialTextExtractor16.png",
      "32": "images/SocialTextExtractor32.png",
      "48": "images/SocialTextExtractor48.png",
      "128": "images/SocialTextExtractor128.png"
    }
  },
  "icons": {
    "16": "images/SocialTextExtractor16.png",
    "32": "images/SocialTextExtractor32.png",
    "48": "images/SocialTextExtractor48.png",
    "128": "images/SocialTextExtractor128.png"
  }
}
