{
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "version": "1.0",
  "manifest_version": 3,
  "default_locale": "en_US",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["content_scripts/blur.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": [],
